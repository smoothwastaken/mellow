{
  "expo": {
    "name": "mellow",
    "slug": "mellow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "plugins": [
      [
        "expo-image-picker"
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/394a1043-5a60-4333-8632-de1bc30bc203"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "mellow",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cleeryy.mellow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
